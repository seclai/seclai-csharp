{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "src/Seclai/Seclai.csproj"
          ]
        }
      ],
      "dest": "build/api",
      "disableGitFeatures": true
    }
  ],
  "build": {
    "content": [
      {
        "src": "docs",
        "files": [
          "**/*.md",
          "toc.yml"
        ]
      },
      {
        "files": [
          "api/index.md"
        ]
      },
      {
        "src": "build/api",
        "dest": "api",
        "files": [
          "**/*.yml"
        ]
      }
    ],
    "resource": [
      {
        "src": "docs",
        "files": [
          "images/**"
        ]
      }
    ],
    "globalMetadata": {
      "_appTitle": "Seclai C# SDK",
      "_enableSearch": true
    },
    "dest": "build/docs",
    "template": [
      "default"
    ]
  }
}
